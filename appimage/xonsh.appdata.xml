<?xml version="1.0" encoding="UTF-8"?>
<component type="desktop-application">
    <id>deepsh</id>
    <metadata_license>BSD</metadata_license>
    <project_license>BSD</project_license>
    <name>Deepsh</name>
    <summary>Deepsh on Python {{ python-fullversion }}</summary>
    <description>
        <p>  Python {{ python-fullversion }} + Deepsh bundled in an AppImage.
        </p>
    </description>
    <launchable type="desktop-id">deepsh.desktop</launchable>
    <url type="homepage">http://con.sh</url>
    <provides>
        <binary>python{{ python-version }}</binary>
    </provides>
</component>
